</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_parallelogram_set/shape">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_parallelogram_set/shape</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             A qualitative description of each parallelogram/rectangle/square/box.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_parallelogram_set.html#nxcg-parallelogram-set-shape-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_parallelogram_set/length">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_parallelogram_set/length</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Qualifier how one edge is longer than all the other edge of the parallelogam.
             Often the term length is associated with one edge being parallel to
             an axis of the coordinate system.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_parallelogram_set.html#nxcg-parallelogram-set-length-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_parallelogram_set/width">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_parallelogram_set/width</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Qualifier often used to describe the length of an edge within
             a specific coordinate system.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_parallelogram_set.html#nxcg-parallelogram-set-width-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_parallelogram_set/center">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_parallelogram_set/center</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Position of the geometric center, which often is but not necessarily
             has to be the center_of_mass of the parallelogram.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_parallelogram_set.html#nxcg-parallelogram-set-center-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_parallelogram_set/surface_area">
