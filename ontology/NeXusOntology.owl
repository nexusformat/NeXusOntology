</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_grid/origin">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_grid/origin</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comment>
