<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXapm_paraprobe_config_intersector/ENTRY/PROCESS/PROCESS/analyze_coprecipitation">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
