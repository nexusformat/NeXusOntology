  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXapm_paraprobe_config_intersector/ENTRY/element_whitelist</rdfs:label>
