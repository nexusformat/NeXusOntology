  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/width</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Qualifier often used to describe the length of an edge within
             a specific coordinate system.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-width-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/height">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/height</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Qualifier often used to describe the length of an edge within
             a specific coordinate system.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-height-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/center">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/center</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Position of the geometric center, which often is but not necessarily
             has to be the center_of_mass of the hexahedrally-shaped sample/sample part.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-center-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/volume">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/volume</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"></rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-volume-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/surface_area">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/surface_area</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Total area (of all six faces) of each hexahedron.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-surface-area-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/face_area">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/face_area</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Area of each of the six faces of each hexahedron.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-face-area-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/is_box">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/is_box</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Specifies if the hexahedra represent cuboids or cubes eventually rotated
             ones but at least not too exotic six-faced polyhedra.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-is-box-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/is_axis_aligned">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/is_axis_aligned</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Only to be used if is_box is present. In this case, this field describes
             whether hexahedra are boxes whose primary edges are parallel to the
             axes of the Cartesian coordinate system.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-is-axis-aligned-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/identifier_offset">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/identifier_offset</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Integer which specifies the first index to be used for distinguishing
             hexahedra. Identifiers are defined either implicitly
             or explicitly. For implicit indexing the identifiers are defined on the
             interval [identifier_offset, identifier_offset+c-1].
             For explicit indexing the identifier array has to be defined.
             
             The identifier_offset field can for example be used to communicate if the
             identifiers are expected to start from 1 (referred to as Fortran-/Matlab-)
             or from 0 (referred to as C-, Python-style index notation) respectively.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-identifier-offset-field</rdfs:seeAlso>
</owl:Class>

<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXcg_hexahedron_set/identifier">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXcg_hexahedron_set/identifier</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
             Integer used to distinguish hexahedra for explicit indexing.
        </rdfs:comment>
  <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://fairmat-experimental.github.io/nexus-fairmat-proposal/1c3806dba40111f36a16d0205cc39a5b7d52ca2e/classes/contributed_definitions/NXcg_hexahedron_set.html#nxcg-hexahedron-set-identifier-field</rdfs:seeAlso>
</owl:Class>

