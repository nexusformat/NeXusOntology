  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXpulser_apm/pulse_mode</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
