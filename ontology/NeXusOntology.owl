<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXapm_paraprobe_config_surfacer/ENTRY/PROCESS/surface_meshing/has_closure">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NXapm_paraprobe_config_surfacer/ENTRY/PROCESS/surface_meshing/has_closure</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">
