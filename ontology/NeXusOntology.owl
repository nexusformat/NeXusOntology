<owl:Class rdf:about="http://purl.org/nexusformat/definitions/Field/NXapm_paraprobe_config_spatstat/ENTRY/spatial_statistics/ion_to_feature_distances/randomize_labels">
  <rdfs:subClassOf rdf:resource="#NeXusField"/>
